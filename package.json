{
  "name": "whineApp",
  "version": "2.0.0",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "babel",
      "react"
    ],
    "extends": "eslint:recommended",
    "browser": true,
    "rulues": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "react/jsx-uses-react": 1
    }
  },
  "scripts": {
    "babel": "babel app/frontends -t babelify -o app/frontends/build/app.js",
    "browserify": "browserify app/frontends/build/app.js -t babelify -o public/assets/javascripts/bundle.js"
  },
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "eslint": "^3.18.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^6.10.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "watch": "^1.0.2"
  }
}
